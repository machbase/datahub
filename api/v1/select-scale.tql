SQL(strSprintf(`SELECT name, min(value) as min, max(value) as max FROM %s
    WHERE %s and time between '%s' and  '%s' group by name order by name`,
                param('table') ?? 'home',
                ((param('name') == NULL) || (param('name') == '')) ? 'name is not null' : (strSprintf("name in (%s)", param('name'))),
                param('start') ?? '2016-01-01 14:00:00',
                param('end')   ?? '2016-01-01 14:00:10'
    )
)
CSV(header(true))
