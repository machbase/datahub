SQL(strSprintf(`select name as NAME, min(min2) as MIN, max(max2) as MAX from (
    SELECT name, rollup('day', 1, time) as mtime, max(value) as max2, min(value) as min2 FROM %s 
    WHERE name in (%s) and time between %s and  %s group by name, mtime order by name, mtime) group by name`,                 
                param('table') ?? 'home', 
                param('name') ?? "'TAG-pressure'",
                param('start') ?? 'FROM_UNIXTIME(1451624400)', 
                param('end')   ?? 'FROM_UNIXTIME(1562128280)'))
CSV()